{
  "name": "unlisted-trading-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:tablet": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "ai:install": "cd ai-service && pip install -r requirements.txt",
    "ai:dev": "cd ai-service && python app.py",
    "ai:prod": "cd ai-service && gunicorn -w 4 -b 0.0.0.0:5001 app:app",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run ai:dev\" \"npm run dev\"",
    "migrate": "knex migrate:latest --knexfile server/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile server/knexfile.js",
    "seed": "knex seed:run --knexfile server/knexfile.js",
    "test": "jest",
    "vercel-build": "npm run migrate && npm run seed && next build",
    "postinstall": "npm run migrate",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.1",
    "@supabase/supabase-js": "^2.76.1",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "next": "16.0.0",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-redux": "^9.2.0",
    "recharts": "^3.3.0",
    "sharp": "^0.34.4",
    "tesseract.js": "^6.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-jest": "^30.2.0",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
